keycode  49 = l L l L
keycode  64 = BackSpace BackSpace BackSpace BackSpace
keycode  67 = Escape NoSymbol Escape
keycode  37 = backslash bar backslash bar
keycode  78 = F2 F2 F2 F2 F2 F2 XF86Switch_VT_2

clear control
add control   = Control_L Control_R

clear mod1
add mod1      = Alt_L Meta_L Alt_R Meta_R


!add shift = Shift_L Shift_R
!add lock      = Caps_Lock

!clear mod2
!add mod2      = Num_Lock

!clear mod4
!add mod4      = Super_L Super_R Super_L Hyper_L

!clear mod5
!add mod5      = ISO_Level3_Shift  Mode_switch
